{
    "name": "buzzingpixel/corbomite-mailer",
    "version": "1.0.0",
    "description": "Corbomite Mailer",
    "authors": [
        {
            "name": "TJ Draper",
            "email": "tj@buzzingpixel.com",
            "homepage": "https://buzzingpixel.com"
        }
    ],
    "require": {
        "php": ">=7.1",
        "buzzingpixel/corbomite-di": "^1.0",
        "buzzingpixel/corbomite-queue": "^1.0",
        "buzzingpixel/corbomite-db": "^1.0",
        "sendgrid/sendgrid": "^7.2",
        "mandrill/mandrill": "^1.0",
        "mailgun/mailgun-php": "^2.8",
        "wildbit/postmark-php": "^2.6"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "squizlabs/php_codesniffer": "^3.4",
        "phpmd/phpmd": "^2.6",
        "twig/twig": "^2.6",
        "phpunit/phpunit": "^7.5",
        "buzzingpixel/corbomite-cli": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "buzzingpixel\\corbomitemailer\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "buzzingpixel\\tests\\": "tests/"
        }
    },
    "extra": {
        "diConfigFilePath": "src/diConfig.php"
    }
}
